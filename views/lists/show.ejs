
<% list.gifts.forEach(gift => { %>

  <div class="container-fluid">
  <div class="row">
  <div class="col-md-4">
  <div class="card">
  <div class="card-body">
  <h4 class="card-header">For: <%= gift.name %></h4>
  <a href="/lists/<%= list.id %>/gifts/<%= gift.id %> " class="btn btn-primary"><i class="fa fa-eye" aria-hidden="true"></i> gift</a>
  </div>
  </div>
  </div>
  </div>
  </div>
  <% }) %>
  <% if(locals.isLoggedIn && list.belongsTo(user)) { %>
    <div class="container-fluid">
    <div class="row">
    <div class="col-md-4">
    <div class="card">
    <div class="card-body">
    <h4><a href="/lists/<%= list.id %>/edit" class="btn btn-primary"><i class="fa fa-pencil" aria-hidden="true"></i> list</a></h4>
    <a href="/lists/<%= list.id %>/gifts/new" class="btn btn-primary"><i class="fa fa-plus" aria-hidden="true"></i> gift</a>
    <% } %>
    <a href="/lists" class="btn btn-primary"><i class="fa fa-arrow-left" aria-hidden="true"></i> to lists</a>
  </div>
</div>
</div>
</div>
</div>
