


<div class="container">
  <div class="row">
  <% users.forEach(user => { %>
  <div class="col-3 ">
  <div class="card">
  <h4 class="card-header"><%= user.username %></h4>
  <a class="card-body" href="/users/<%= user.id %>"><i class="fa fa-eye" aria-hidden="true"></i> See their list</a>

</div>
</div>
<% }) %>
</div>
</div>
